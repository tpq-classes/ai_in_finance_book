      "source": [
        "def cw(df):\n",
        "    c0, c1 = np.bincount(df['d'])\n",
        "    w0 = (1 / c0) * (len(df)) / 2\n",
        "    w1 = (1 / c1) * (len(df)) / 2\n",
        "    return {0: w0, 1: w1}"
      ]
    },
