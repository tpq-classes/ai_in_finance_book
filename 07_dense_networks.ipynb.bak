      "outputs": [],
      "source": [
        "from keras.regularizers import l1, l2"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "aRHeXv3dwaub"
      },
      "outputs": [],
      "source": [
        "def create_model(hl=1, hu=128, dropout=False, rate=0.3,\n",
        "                 regularize=False, reg=l1(0.0005),\n",
        "                 optimizer=optimizer, input_dim=len(cols)):\n",
        "    if not regularize:\n",
        "        reg = None\n",
        "    model = Sequential()\n",
        "    model.add(Dense(hu, input_dim=input_dim,\n",
        "                    activity_regularizer=reg,\n",
        "                    activation='relu'))\n",
        "    if dropout:\n",
        "        model.add(Dropout(rate, seed=100))\n",
        "    for _ in range(hl):\n",
        "        model.add(Dense(hu, activation='relu',\n",
        "                        activity_regularizer=reg))\n",
        "        if dropout:\n",
        "            model.add(Dropout(rate, seed=100))\n",
        "    model.add(Dense(1, activation='sigmoid'))\n",
        "    model.compile(loss='binary_crossentropy', optimizer=optimizer,\n",
        "                 metrics=['accuracy'])\n",
        "    return model"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "q3aUpsgMwaub"
